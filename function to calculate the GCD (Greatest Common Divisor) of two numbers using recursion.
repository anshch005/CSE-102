#include <stdio.h>

// Recursive function to calculate GCD
int gcd(int a, int b) {
    // Base case: if b is 0, return a
    if (b == 0) {
        return a;
    }
    // Recursive case: gcd(b, a % b)
    return gcd(b, a % b);
}

int main() {
    int a, b;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    printf("GCD is %d\n", gcd(a, b));

    return 0;
}
